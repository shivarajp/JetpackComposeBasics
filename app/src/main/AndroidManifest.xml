<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shivaraj.composetaskmanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeTaskManager">
        <activity
            android:name="com.shivaraj.composetaskmanager.ComposeVolumeButton"
            android:exported="true"
            android:label="@string/title_activity_compose_volume_button"
            android:theme="@style/Theme.ComposeTaskManager">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shivaraj.composetaskmanager.ComposeAnimatedProgressBar"
            android:exported="true"
            android:label="@string/title_activity_compose_animated_progress_bar"
            android:theme="@style/Theme.ComposeTaskManager">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shivaraj.composetaskmanager.ComposeAnimation"
            android:exported="true"
            android:label="@string/title_activity_compose_animation"
            android:theme="@style/Theme.ComposeTaskManager">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shivaraj.composetaskmanager.ComposeCpnstraintLayout"
            android:exported="true"
            android:label="@string/title_activity_compose_cpnstraint_layout"
            android:theme="@style/Theme.ComposeTaskManager">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shivaraj.composetaskmanager.ListComposeActivity"
            android:exported="true"
            android:label="@string/title_activity_list_compose"
            android:theme="@style/Theme.ComposeTaskManager">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.shivaraj.composetaskmanager.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeTaskManager">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>